{
  "name": "migortig-server",
  "version": "0.0.2",
  "description": "server for migortig",
  "enginers": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnichols08/migortig.git"
  },
  "keywords": [
    "node",
    "react"
  ],
  "author": "Mikey Nichols",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mnichols08/migortig/issues"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "stripe": "^8.153.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  }
}
