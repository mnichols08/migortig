{
  "name": "migortig-server",
  "version": "0.0.3",
  "description": "server for migortig",
  "engines": {
    "node": "16.2.0",
    "npm": "7.13.0"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && yarn run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "test-client": "cd client && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnichols08/migortig.git"
  },
  "keywords": [
    "node",
    "react"
  ],
  "author": "Mikey Nichols",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mnichols08/migortig/issues"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "stripe": "^8.153.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7",
    "react-scripts": "^4.0.3",
    "react-dom": "^17.0.2"
  }
}
